Procedure MainWindow_Events(event)
  Select event
    Case #PB_Event_CloseWindow
      ProcedureReturn #False

    Case #PB_Event_Menu
      Select EventMenu()
        Case #MenuItem_2
          CreateFile_Database(EventMenu())
        Case #MenuItem_3
        Case #MenuItem_4
        Case #MenuItem_5
        Case #MenuItem_7
        Case #MenuItem_9
      EndSelect

    Case #PB_Event_Gadget
      Select EventGadget()
        Case #BT_Save
          Save(EventType())          
        Case #BT_Delete
          Delete(EventType())          
        Case #BT_Insert
          Insert(EventType())          
        Case #BT_Search
          Search(EventType())          
      EndSelect
  EndSelect
  ProcedureReturn #True
EndProcedure

