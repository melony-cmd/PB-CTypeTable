Global WindowConvert


Procedure OpenWindowConvert(x = 0, y = 0, width = 475, height = 190)
  WindowConvert = OpenWindow(#PB_Any, x, y, width, height, "Convert - PBC Type Table", #PB_Window_SystemMenu | #PB_Window_ScreenCentered | #PB_Window_WindowCentered)
EndProcedure

Procedure WindowConvert_Events(event)
  Select event
    Case #PB_Event_CloseWindow
      ProcedureReturn #False

    Case #PB_Event_Menu
      Select EventMenu()
      EndSelect

    Case #PB_Event_Gadget
      Select EventGadget()
      EndSelect
  EndSelect
  ProcedureReturn #True
EndProcedure

